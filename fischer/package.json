{
  "name": "@chess/fischer",
  "scripts": {
    "start": "ts-node src/app",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev src/app.ts --transpile-only --rs",
    "test": "jest --silent --colors --runInBand",
    "test:ci": "jest --silent --ci --runInBand",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "cruise-deps": "depcruise \"src/**/*.{js,ts,tsx}\"",
    "cm": "yarn --cwd ../ cm",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "node-cache": "5.1.2",
    "socket.io": "4.1.2",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.171",
    "@types/node": "16.0.0",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "cross-env": "7.0.3",
    "dependency-cruiser": "10.0.4",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.8.0-125",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "socket.io-client": "4.1.2",
    "standard-version": "9.3.0",
    "ts-jest": "27.0.3",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5"
  }
}
